<div class="card-content">
{{#each .}}
<a href="{{ url }}" data-mmc-link="{{ url }}">
<div class="row">
    <div class="col-xs-3">
        <img src="/images/video-title/{{ id }}?width=160&height=90" />
    </div>
    <div class="col-xs-9">
        <span class="video-title">{{ title }} {{#if year}}<small>{{ year }}</small>{{/if}}</span>
    </div>
</div>
</a>
{{/each}}
</div>
