<script type="text/javascript">
Memocast.locale = '{{ locale }}';
$(function () {
  $('[data-toggle="tooltip"]').tooltip()
});
Memocast.LegacyPlayback = {{#if legacyPlayback}}true{{else}}false{{/if}};
</script>
{{#if user }}
<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/1.7.2/socket.io.min.js"></script>
<script type="text/javascript">
Memocast.CurrentUserID = '{{ user.id }}';
Memocast.CurrentUserIsAdmin = {{#if userIsAdmin}}true{{/if}}{{#unless userIsAdmin}}false{{/unless}};
var SocketIOAddress = '{{ socketio }}';
var videoTitleID = '';
var SocketEstablishedHandler = null;
</script>
{{/if}}

